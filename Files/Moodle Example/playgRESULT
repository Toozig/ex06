int a; ---> Variable
int y; ---> Variable
 ---> Emptyline
void goo(int b){ ---> MethodDeclare
    y = a; ---> VariableAssignment
    if(b){ ---> IfWhileBlock
    return; ---> Return
    } ---> ScopeClosing
    return; ---> Return
} ---> ScopeClosing
 ---> Emptyline
void boo(){ ---> MethodDeclare
    goo("ggg"); ---> MethodCall
} ---> ScopeClosing
